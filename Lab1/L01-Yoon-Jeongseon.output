old:SELECT 
    TO_CHAR(&tomorrow, 'Month') || ' ' || 
    TO_CHAR(&tomorrow, 'ddth') || ' of year ' || 
    TO_CHAR(&tomorrow, 'YYYY') AS "Tomorrow" 
FROM dual
new:SELECT 
    TO_CHAR(SYSDATE + 1, 'Month') || ' ' || 
    TO_CHAR(SYSDATE + 1, 'ddth') || ' of year ' || 
    TO_CHAR(SYSDATE + 1, 'YYYY') AS "Tomorrow" 
FROM dual

