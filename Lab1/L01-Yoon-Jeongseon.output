old:SELECT product_id AS "Product ID", product_name AS "Product Name", list_price,
    &newListPrice AS "New Price",
    &newListPrice - list_price AS "Price Difference"
FROM products
WHERE category_id in (2, 3, 5)
ORDER BY category_id, product_id
new:SELECT product_id AS "Product ID", product_name AS "Product Name", list_price,
    ROUND(list_price + list_price * 0.02) AS "New Price",
    ROUND(list_price + list_price * 0.02) - list_price AS "Price Difference"
FROM products
WHERE category_id in (2, 3, 5)
ORDER BY category_id, product_id

